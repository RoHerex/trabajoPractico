test "objMartin"{
	
	
	martin.cantidadDeVecesJugadas()
	martin.energia()
	martin.reducirEnergia(cantidadDeEnergia)
	martin.frutaComprada()
	martin.hacerDeporte(deporte)
	martin.nuevoAnio()
	assert.that(martin.esAdulto())
	assert.that(martin.tieneBajaEnergia())
	assert.that(martin.estaFeliz())
	martin.comprarFruta(unaFruta)
	martin.hacerMadurarALaFruta()
	martin.comer()
	assert.that(martin.esDeportista())
}     

